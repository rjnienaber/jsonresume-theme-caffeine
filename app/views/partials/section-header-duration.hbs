<div class="section-header clearfix">
  {{#if name}}
    <h3 class="bold pull-left">
      {{#if website}}
        <a href="{{website}}" target="_blank">
          {{name}} <i class="fa" aria-hidden="true"></i>
        </a>
      {{else}}
        {{name}}
      {{/if}}
    </h3>
    {{#if positionType}}
      <h5><sup>({{positionType}})</sup></h5>
    {{/if}}
  {{/if}}
</div>
